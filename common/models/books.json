{
  "name": "books",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "author": {
      "type": "string",
      "required": true
    },
    "publisher": {
      "type": "string",
      "required": true,
      "default": "SuperNowa"
    },
    "publishYear": {
      "type": "number",
      "required": true
    },
    "type": {
      "type": "string",
      "required": true
    },
    "pages": {
      "type": "number",
      "required": true
    },
    "pictures": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
